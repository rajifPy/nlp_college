{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key",
    "MODEL_PATH": "/tmp/models",
    "MODEL_URL": "@model_url"
  },
  "buildCommand": "pip install -r requirements.txt"
}